<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="br.unb.mobileMedia.MMShare" android:versionCode="1" android:versionName="1.0">

    <uses-sdk android:minSdkVersion="16"/>

    <!-- Indicam que a aplica&#231;&#227;o tem acesso ao envio e leitura de SMS -->
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission android:name="android.permission.READ_SMS"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application android:icon="@drawable/ic_launcher" android:label="@string/app_name">
        
        <activity android:name=".MMUnBActivity" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        
        
        
        <!-- FEATURE BEGIN SHARE -->
        <activity android:name=".core.view.ShareListActivity" android:label="@string/title_activity_share_list">
        </activity>
        <!-- FEATURE END SHARE -->
        
        
    </application>
</manifest>